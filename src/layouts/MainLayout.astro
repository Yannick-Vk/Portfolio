---
import '../styles/global.css';
import Navbar from "@/components/Navbar.tsx";
const { title } = Astro.props;
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<title>{title}</title>
	</head>
	<body>
	<header>
		<Navbar client:load />
	</header>
	<main>
		<slot />
	</main>
	</body>
</html>
